<h1>Listing licenses</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Location desc</th>
      <th>Registration</th>
      <th>Industry type</th>
      <th>Fish type</th>
      <th>Date issued</th>
      <th>Date expires</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @licenses.each do |license| %>
      <tr>
        <td><%= license.status %></td>
        <td><%= license.location_desc %></td>
        <td><%= license.registration_id %></td>
        <td><%= license.industry_type %></td>
        <td><%= license.fish_type %></td>
        <td><%= license.date_issued %></td>
        <td><%= license.date_expires %></td>
        <td><%= link_to 'Show', license %></td>
        <td><%= link_to 'Edit', edit_license_path(license) %></td>
        <td><%= link_to 'Destroy', license, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New License', new_license_path %>
